{
  "schemaVersion": 1,
  "id": "l2hostility",
  "version": "${version}",
  "name": "L2 Hostility",
  "description": "L2 Hostility",
  "authors": [
    "LightLand Team",
    "Karashok-Leo"
  ],
  "contact": {
    "sources": "https://github.com/Karashok-Leo/L2Hostility-Fabricated"
  },
